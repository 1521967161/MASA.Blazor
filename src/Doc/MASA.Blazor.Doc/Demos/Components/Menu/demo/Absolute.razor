<div style="text-align:center">
	<MMenu Absolute>
		<Slot>
			<MCard>
				<Cover> <img alt="example" src="https://cdn.vuetifyjs.com/images/cards/cooking.png" /> </Cover>
			</MCard>
		</Slot>

		<ChildContent>
			<MList>
				@foreach (var item in titles)
				{
		<MListItem>
		 <MListItemContent>
		  <MListItemTitle>
								@item
		  </MListItemTitle>
		 </MListItemContent>
		</MListItem>
				}
   </MList>
  </ChildContent>
 </MMenu>
</div>

@code {
	string[] titles = {
	"Click Me",
	"Click Me",
	"Click Me",
	"Click Me 2",
	};
}